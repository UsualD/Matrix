
#include <iostream>
#include <ctime>
#include <cstdlib>

using namespace std;

const int SIZE = 5;

void mass(int arr[][SIZE], const int size)
{
for (int i = 0; i < size; i++)
{
for (int j = 0; j < size; j++)
{
arr[i][j] = 30 + rand() % 31;

 cout << arr[i][j] << " ";
}
cout << endl;
}
}
int min(int arr[][SIZE], const int size)
{
int min = arr[0][0];
for (int i = 0; i < size; i++)
{
 for (int j = 0; j < size; j++)
{
if (arr[i][j] < min)
min = arr[i][j];
}
}
return min;
}

int main()
{
setlocale(LC_ALL, "rus");

 int matrix[SIZE][SIZE] = {};
 mass(matrix, SIZE);

 cout << endl;

 cout << "MIN = " << min(matrix, SIZE) << endl;

return 0;
}




